{
  "name": "auto-defi-agent",
  "version": "1.0.0",
  "description": "Smart DeFi Yield Optimization Assistant for BNB Chain",
  "main": "src/main.py",
  "author": "OpenClaw Agent",
  "license": "MIT",
  "openclaw": {
    "skill": true,
